{
    "version": 2,
    "routes": [
      { "src": "/(.*)", "dest": "api.py" }
    ],
    "functions": {
      "api.py": {
        "runtime": "python3.9",
        "memory": 512,
        "maxDuration": 30
      }
    },
    "build": {
      "env": {
        "PYTHON_VERSION": "3.9"
      }
    }
  }
  